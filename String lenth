#include <bits/stdc++.h>
using namespace std;

int main(){
    string str;
    int space = 0;

    cout << "Enter a string: ";
    getline(cin,str);

    for(int i = 0; i < str.length(); i++){
         if(str[i] == ' '){
            space++;
         }
    }

    cout << "Total String length(With space) : " << str.length() << endl;
    cout << "Total String length(Without space) : " << str.length() - space << endl;
}
